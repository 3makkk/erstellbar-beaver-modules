msgid "Speed"
msgstr "Geschwindigkeit"

#: slick-slider/slick-slider.php:354
msgid "Transition speed"
msgstr "Übergangsgeschwindigkeit"

#: slick-slider/slick-slider.php:357
msgid "Swipe"
msgstr "Swipe"

#: slick-slider/slick-slider.php:364
msgid "Enables touch swipe"
msgstr "Aktiviert touch swipe"

#: slick-slider/slick-slider.php:367
msgid "Swipe to swipe"
msgstr "Swipe to swipe"

#: slick-slider/slick-slider.php:377
msgid "Touch Move"
msgstr "touch move"

#: slick-slider/slick-slider.php:384
msgid "Enables slide moving with touch"
msgstr "Aktiviert slide bewegung bei touch"

#: slick-slider/slick-slider.php:387
msgid "Touch threshold"
msgstr "touch Schwellenwert"

#: slick-slider/slick-slider.php:390
msgid ""
"To advance slides, the user must swipe a length of (1/touchThreshold) * the "
"width of the slider."
msgstr ""
"To advance slides, the user must swipe a length of (1/touchThreshold) * the "
"width of the slider."

#: slick-slider/slick-slider.php:393
msgid "Use CSS"
msgstr "Css benutzen"

#: slick-slider/slick-slider.php:400
msgid "Enable/Disable CSS Transitions"
msgstr "Aktiviert/Deaktiviert CSS Transitions"

#: slick-slider/slick-slider.php:410
msgid "Enable/Disable CSS Transforms"
msgstr "Aktiviert/Deaktiviert CSS Transforms"

#: slick-slider/slick-slider.php:413
msgid "Variable width"
msgstr "Variable Breite"

#: slick-slider/slick-slider.php:420
msgid "Disables automatic slide width calculation"
msgstr "Deaktiviert automatische berechnung der Sliderbreite"

#: slick-slider/slick-slider.php:423
msgid "Vertical"
msgstr "Vertikal"

#: slick-slider/slick-slider.php:430
msgid "Vertical slide direction"
msgstr "Vertikale Slide-richtung"

#: slick-slider/slick-slider.php:433
msgid "Vertical swiping"
msgstr "vertikales swipen"

#: slick-slider/slick-slider.php:440
msgid "Changes swipe direction to vertical"
msgstr "Ändert die Swipe-Richtung zu vertikal"

#: slick-slider/slick-slider.php:443
msgid "RTL"
msgstr "RTL"

#: slick-slider/slick-slider.php:450
msgid "Change the slider's direction to become right-to-left"
msgstr "Ändert die Slider-Richgung von rechts auf links"

#: slick-slider/slick-slider.php:453
msgid "Wait for animate"
msgstr "Warte auf animate"

#: slick-slider/slick-slider.php:460
msgid "Ignores requests to advance the slide while animating"
msgstr "Ignoriert Anfragen zur verbesserung des Slides bei Animation"

